
<div class="container">
    <div class="row">
        <div class="col-md-6 col-md-offset-3">
    <h2>Sign in</h2>

    <%= form_tag sessions_path do %>
    
    <%= label_tag :email  %>
    
    <%= text_field_tag :email, params[:email]%>
    

    
    <%= label_tag :password %>
    
    <%= password_field_tag :password %>
    
    
    <%= submit_tag "Sign in", class: "btn btn-lg btn-primary" %>

 <% end %> 
 </div>
 </div>
 </div>


